//static are those whose value remains same for all objects

class Student{
    String name;
    static String school;
    
}

public class Static{
    public static void main(String args[]){
        Student.school="CMS";      //accessed using class name bcoz it belongs to class
        Student s1=new Student();
        s1.name="Ajay";
        System.out.println(s1.school);
        
    }
}
